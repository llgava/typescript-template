{
  "name": "typescript-template",
  "version": "1.0.0",
  "main": "./build/index.js",
  "description": "A small description about the project.",
  "keywords": [
    "JavaScript",
    "TypeScript"
  ],
  "author": "llgava <llgavamt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "build": "babel ./src --extensions \".js,.ts\" --out-dir ./build --copy-files --no-copy-ignored",
    "start": "node .",
    "bump": "yarn upgrade --latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.14.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llgava/typescript-template.git"
  },
  "dependencies": {}
}
